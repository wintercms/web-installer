{
    "name": "wintercms/web-installer",
    "description": "Web-based installer for Winter CMS",
    "type": "project",
    "require": {
        "winter/packager": "^0.1.5",
        "winter/laravel-config-writer": "^1.0.0",
        "doctrine/dbal": "^3.1.4",
        "illuminate/database": "^9.20.0",
        "monolog/monolog": "^2.7.0"
    },
    "autoload": {
        "psr-4": {
            "Winter\\Installer\\": "src"
        }
    },
    "config": {
        "platform": {
            "php": "8.0.2"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
